<%= form_with(model: user, local: true) do |form| %>
  <% if user.errors.any? %>
    <div id="error-explanation">
      <h2><%= pluralize(user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :picture %>
    <%= form.filestack_field :picture , 'Upload', input_id: 'unique_id2', pickerOptions: { 'maxFiles': 20, 'storeTo': {'location' => 's3' }, onFileUploadFinished: 'onFileUploadFinishedCallback' }, required: true, multiple: true %>
  </div>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn' %>
  </div>
<% end %>
